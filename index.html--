function calTime(objTimeStr)
{
	var timeArr = new Array();
	timeArr = objTimeStr.split(",");
	var objdateyear = timeArr[0];
	var objdatemonth = timeArr[1];
	var objdatedate = timeArr[2];
	var objdatehours = timeArr[3];
	var objdatemin = timeArr[4];
	var objdatesec = timeArr[5];
	var objdate = new Date(objdateyear,objdatemonth,objdatedate,objdatehours,objdatemin,objdatesec);
	
	var nowdate = new Date();
	var nowyear = nowdate.getFullYear();
	var nowmonth = nowdate.getMonth() + 1;
	var nowday = nowdate.getDate();
	var nowhours = nowdate.getHours();
	var nowmin = nowdate.getMinutes();
	var nowsec= nowdate.getSeconds();
	
	if (nowyear > objdateyear)
	{
		return nowyear - objdateyear + " year ago";
	}
	else if (nowmonth > objdatemonth)
	{
		return nowmonth - objdatemonth + " month ago";
	}
	else if (nowday > objdatedate)
	{
		return nowday - objdatedate + " day ago";
	}
	else if (nowhours > objdatehours)
	{
		return nowhours - objdatehours + " hours ago";
	}
	else if (nowmin > objdatemin)
	{
		return nowmin - objdatemin + " minutes ago";
	}
	else if (nowsec > objdatesec)
	{
		return nowsec - objdatesec + " seconds ago";
	}
	else
	{
		return "just now";
	}
}
